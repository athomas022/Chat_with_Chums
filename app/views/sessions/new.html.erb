<body>
  <%= render 'partials/navbar' %>
<h2>Log in</h2>
<%= form_with url: login_path, method: :post, local: true do |f| %>
  <div>
    <%= f.text_field :username, label: 'Username',  placeholder: 'Your username here' %>
  </div>
  <div>
    <%= f.password_field :password, label: 'Password',  autocomplete: "username" %>
  </div>
  <%= f.submit "Login in" %>
  <%= link_to 'Sign up', new_user_path %>
<% end %>
<%= render 'partials/footer' %>
</body>